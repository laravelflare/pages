            "email": "aden@jacobbailey.com",
            "homepage": "https://github.com/AdenFraser"
        }
    ],
    "require": {
        "laravelflare/flare": "^0.9",
        "laravelflare/cms": "^0.1"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.1",
        "phpunit/phpunit": "~4.0",
        "orchestra/testbench": "~3.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "autoload": {
