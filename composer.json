{
    "name": "laravelflare/pages",
    "description": "Pages Module for Flare CMS",
    "keywords": [
        "Laravel", "Flare", "CMS", "Pages", "Module"
    ],
    "homepage": "https://github.com/laravelflare/pages",
    "license": "MIT",
    "authors": [
        {
            "name": "Laravel Flare",
            "homepage": "https://github.com/laravelflare"
        },
        {
